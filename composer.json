{
    "name": "activenetwork/aus-soap-api",
    "type": "library",
    "description": "PHP classes based on ACTIVE Network's User Service SOAP WSDL",
    "keywords": ["ACTIVE", "Active Network", "User Service", "SOAP", "API"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Rhyan Randolph",
            "email": "rhyan.randolph@activenetwork.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "vlucas/phpdotenv": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "wsdl2phpgenerator/wsdl2phpgenerator": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Active\\": "src/Active"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
